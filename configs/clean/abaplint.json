{
    "global": {
        "files": "/src/**/*.*",
        "skipGeneratedGatewayClasses": true,
        "skipGeneratedPersistentClasses": true,
        "skipGeneratedFunctionGroups": true,
        "applyUnspecifiedRules": false
    },
    "syntax": {
        "version": "v753",
        "errorNamespace": "",
        "globalConstants": [],
        "globalMacros": []
    },
    "dependencies": [],
    "rules": {
        "avoid_use": {
            "define": true,
            "endselect": true,
            "execSQL": true,
            "kernelCall": true,
            "communication": true,
            "statics": true
        },
        "breakpoint": {},
        "commented_code": {},
        "constructor_visibility_public": {},
        "description_empty": {},
        "empty_statement": {},
        "empty_structure": {
            "loop": true,
            "if": true,
            "while": true,
            "case": true,
            "select": true,
            "do": true,
            "at": true
        },
        "exporting": {},
        "functional_writing": {
            "ignoreExceptions": true
        },
        "global_class": {},
        "identical_form_names": {},
        "if_in_if": {},
        "inline_data_old_versions": {},
        "line_length": {
            "length": 120
        },
        "max_one_statement": {},
        "message_exists": {},
        "method_length": {
            "statements": 25
        },
        "mix_returning": {},
        "msag_consistency": {},
        "nesting": {
            "depth": 5
        },
        "obsolete_statement": {
            "refresh": true,
            "compute": true,
            "add": true,
            "subtract": true,
            "multiply": true,
            "move": true,
            "divide": true,
            "requested": true
        },
        "parser_error": {},
        "preferred_compare_operator": {
            "badOperators": [
                "EQ",
                "NE",
                "GE",
                "GT",
                "LT",
                "LE"
            ]
        },
        "short_case": {
            "length": 1,
            "allow": []
        },
        "superclass_final": {},
        "unreachable_code": {},
        "use_new": {},
        "when_others_last": {},
        "ambiguous_statement": {},
        "begin_end_names": {},
        "check_transformation_exists": {},
        "check_variables": {},
        "form_tables_obsolete": {},
        "tabl_enhancement_category": {},
        "implement_methods": {},
        "local_testclass_location": {},
        "main_file_contents": {}
    }
}