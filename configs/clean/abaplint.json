{
    "global": {
        "files": "/src/**/*.*",
        "skipGeneratedGatewayClasses": true,
        "skipGeneratedPersistentClasses": true,
        "skipGeneratedFunctionGroups": true,
        "applyUnspecifiedRules": false
    },
    "syntax": {
        "version": "v753",
        "errorNamespace": "",
        "globalConstants": [],
        "globalMacros": []
    },
    "dependencies": [],
    "rules": {
        "avoid_use": {
            "define": true,
            "endselect": true,
            "execSQL": true,
            "kernelCall": true,
            "communication": true,
            "statics": true
        },
        "breakpoint": {},
        "commented_code": {},
        "constructor_visibility_public": {
            "reason": "https://github.com/SAP/styleguides/blob/master/clean-abap/CleanABAP.md#if-your-global-class-is-create-private-leave-the-constructor-public"
        },
        "description_empty": {},
        "empty_statement": {},
        "empty_structure": {
            "loop": true,
            "if": true,
            "while": true,
            "case": true,
            "select": true,
            "do": true,
            "at": true
        },
        "exporting": {
            "reason": "https://github.com/SAP/styleguides/blob/master/clean-abap/CleanABAP.md#omit-the-optional-keyword-exporting"
        },
        "functional_writing": {
            "ignoreExceptions": true
        },
        "global_class": {},
        "identical_form_names": {},
        "if_in_if": {
            "reason": "https://github.com/SAP/styleguides/blob/master/clean-abap/CleanABAP.md#keep-the-nesting-depth-low"
        },
        "inline_data_old_versions": {},
        "line_length": {
            "length": 120,
            "reason": "https://github.com/SAP/styleguides/blob/master/clean-abap/CleanABAP.md#stick-to-a-reasonable-line-length"
        },
        "max_one_statement": {
            "reason": "https://github.com/SAP/styleguides/blob/master/clean-abap/CleanABAP.md#no-more-than-one-statement-per-line"
        },
        "message_exists": {},
        "method_length": {
            "statements": 25,
            "reason": "https://github.com/SAP/styleguides/blob/master/clean-abap/CleanABAP.md#keep-methods-small"
        },
        "mix_returning": {
            "reason": "https://github.com/SAP/styleguides/blob/master/clean-abap/CleanABAP.md#use-either-returning-or-exporting-or-changing-but-not-a-combination"
        },
        "msag_consistency": {},
        "nesting": {
            "depth": 5,
            "reason": "https://github.com/SAP/styleguides/blob/master/clean-abap/CleanABAP.md#keep-the-nesting-depth-low"
        },
        "obsolete_statement": {
            "refresh": true,
            "compute": true,
            "add": true,
            "subtract": true,
            "multiply": true,
            "move": true,
            "divide": true,
            "requested": true
        },
        "parser_error": {},
        "preferred_compare_operator": {
            "badOperators": [
                "EQ",
                "NE",
                "GE",
                "GT",
                "LT",
                "LE"
            ]
        },
        "short_case": {
            "length": 1,
            "allow": []
        },
        "superclass_final": {},
        "unreachable_code": {},
        "use_new": {},
        "when_others_last": {},
        "ambiguous_statement": {},
        "begin_end_names": {},
        "check_transformation_exists": {},
        "check_variables": {},
        "form_tables_obsolete": {},
        "tabl_enhancement_category": {},
        "implement_methods": {},
        "local_testclass_location": {},
        "main_file_contents": {},
        "rfc_error_handling": {},
        "indentation": {
            "alignTryCatch": false,
            "ignoreExceptions": false,
            "globalClassSkipFirst": false,
            "ignoreGlobalClassDefinition": false,
            "ignoreGlobalInterface": false
        },
        "sequential_blank": {
            "lines": 3
        },
        "empty_line_in_statement": {
            "allowChained": true
        }
    }
}